{
  "name": "@aacitelli/credit-card-bonuses",
  "version": "1.0.0",
  "scripts": {
    "dev": "sls offline start --stage offline",
    "deploy:prod": "sls deploy --stage prod",
    "lint": "tsc --noEmit",
    "test": "jest",
    "ci": "pnpm lint && pnpm test",
    "generate": "swagger-cli validate src/api.yaml && mkdirp src/generated &&  openapi-zod-client ./src/api.yaml --output src/generated/api.client.ts --export-schemas",
    "prepare": "husky install",
    "actions": "act --secret-file .env"
  },
  "dependencies": {
    "@zodios/core": "^10.9.0",
    "date-fns": "^2.29.3",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "helmet": "^7.0.0",
    "serverless-http": "^3.2.0",
    "zod": "^3.21.4"
  },
  "packageManager": "pnpm@8.6.0",
  "devDependencies": {
    "@apidevtools/swagger-cli": "^4.0.4",
    "@swc/core": "^1.3.51",
    "@swc/jest": "^0.2.26",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.0",
    "@types/node": "18.16.19",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.58.0",
    "@typescript-eslint/parser": "^5.58.0",
    "esbuild": "~0.18.0",
    "eslint": "^8.38.0",
    "eslint-config-next": "^13.3.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-define-config": "^1.18.0",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-unicorn": "^47.0.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "mkdirp": "^3.0.0",
    "openapi-zod-client": "^1.6.1",
    "prettier": "^2.8.7",
    "serverless": "^3.30.1",
    "serverless-esbuild": "^1.43.1",
    "serverless-offline": "^12.0.4",
    "serverless-plugin-datadog": "^5.25.0",
    "supertest": "^6.3.3",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4"
  },
  "lint-staged": {
    "src/api.yaml": [
      "swagger-cli validate"
    ],
    "*.{ts,tsx}": [
      "eslint --fix",
      "eslint"
    ]
  }
}
