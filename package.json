{
  "name": "@andenacitelli/credit-card-bonuses",
  "type": "module",
  "version": "1.0.0",
  "scripts": {
    "prepare": "husky",
    "format": "prettier . --write",
    "lint": "run-p lint:*",
    "lint:format": "prettier --check .",
    "lint:linter": "oxlint .",
    "lint:typecheck": "tsc --noEmit",
    "lint:openapi": "redocly lint --extends=minimal src/api.yaml",
    "test": "vitest run",
    "generate:schemas": "openapi-zod-client ./src/api.yaml --output src/generated/api.client.ts --export-schemas",
    "generate:exports": "tsx ./bin/generate.ts",
    "generate": "npm run generate:schemas && npm run generate:exports"
  },
  "dependencies": {
    "@zodios/core": "10.9.6",
    "papaparse": "5.5.3",
    "yaml": "2.8.0",
    "zod": "3.25.67"
  },
  "devDependencies": {
    "@redocly/cli": "1.34.3",
    "@tsconfig/recommended": "1.0.10",
    "@types/node": "22.15.2",
    "@types/papaparse": "5.3.16",
    "date-fns": "4.1.0",
    "husky": "9.1.7",
    "lint-staged": "16.1.2",
    "npm-run-all2": "8.0.4",
    "openapi-zod-client": "1.18.3",
    "prettier": "3.5.3",
    "tsx": "4.20.3",
    "typescript": "5.8.3",
    "vite-tsconfig-paths": "5.1.4",
    "vitest": "3.2.4",
    "oxlint": "1.2.0"
  },
  "lint-staged": {
    "*.{ts,json,yml,yaml}": [
      "prettier --write"
    ],
    "*.ts": [
      "oxlint --fix"
    ]
  }
}
